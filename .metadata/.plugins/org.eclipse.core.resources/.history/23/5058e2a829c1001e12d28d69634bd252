
public class Sarrera {
	private  int sarreraid;
	private int data;
	private Pelikula[]pelikulak;
	private int ordutegia;
	private int aretoak;
	private double prezioa;
	
	
	public Sarrera(int sarreraid, int data, Pelikula[] pelikulak, int ordutegia, int aretoak, double prezioa) {
		super();
		this.sarreraid = sarreraid;
		this.data = data;
		this.pelikulak = pelikulak;
		this.ordutegia = ordutegia;
		this.aretoak = aretoak;
		this.prezioa = prezioa;
	}
	
	public int getSarreraid() {
		return sarreraid;
	}
	
	
	public void setSarreraid(int sarreraid) {
		this.sarreraid = sarreraid;
	}
	
	
	public int getData() {
		return data;
	}
	
	
	public void setData(int data) {
		this.data = data;
	}
	
	
	public Pelikula[] getPelikulak() {
		return pelikulak;
	}
	
	
	public void setPelikulak(Pelikula[] pelikulak) {
		this.pelikulak = pelikulak;
	}
	
	
	public int getOrdutegia() {
		return ordutegia;
	}
	
	
	public void setOrdutegia(int ordutegia) {
		this.ordutegia = ordutegia;
	}
	
	
	public int getAretoak() {
		return aretoak;
	}
	
	
	public void setAretoak(int aretoak) {
		this.aretoak = aretoak;
	}
	
	
	public double getPrezioa() {
		return prezioa;
	}
	
	
	public void setPrezioa(double prezioa) {
		this.prezioa = prezioa;
	}
	
	@Override
	public String toString() {
		return "Sarrera [sarreraid=" + sarreraid + ", data=" + data + ", pelikulak=" + pelikulak + ", ordutegia="
				+ ordutegia + ", aretoak=" + aretoak + ", prezioa=" + prezioa + "]";
	}
	
}