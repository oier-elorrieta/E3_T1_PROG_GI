try {
Filma_aukeraketa frame = new Filma_aukeraketa();
frame.setVisible(true);
} catch (Exception e) {
e.printStackTrace();
}
}
});
}

/**
* Create the frame.
*/
public Filma_aukeraketa() {
setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
